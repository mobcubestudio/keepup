<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobcube.keepup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KeepUp">
        <activity
            android:name=".view.activities.PoliticaDePrivacidadeActivity"
            android:exported="true" />
        <activity
            android:name=".view.activities.VeiculoActivity"
            android:exported="true" />
        <activity
            android:name=".view.activities.AddVeiculoActivity"
            android:exported="true" />
        <activity
            android:name=".view.activities.PerfilActivity"
            android:exported="true" />
        <activity
            android:name=".view.activities.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".view.activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/adMobReal" />
    </application>

</manifest>